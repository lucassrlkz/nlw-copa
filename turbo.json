{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },

    "test": { },
    
    "dev":{
      "cache":false
    },
    "start":{
      "cache":false
    },

    "lint": {
      "outputs": []
    },
    
    "deploy": {
      "dependsOn": ["build", "test", "lint"],
      "outputs": []
    }
  }
}